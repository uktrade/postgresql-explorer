{
  "compilerOptions": {
    "strict": false,
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": [
			"es6",
			"es2015.promise",
			"dom"
		],
    "sourceMap": true,
    "rootDir": "src"
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}